decl type_twins(Function, Function);

type_twins(f, g) :- return_type(f, t), return_type(g, t), calls(f, g).

!for_each(type_twins, {
    |(f, g)| {
        println!("type twins: {:?} --> {:?}",
            orig_db.functions[f.0 as usize].1.def_path,
            orig_db.functions[g.0 as usize].1.def_path,
        );
    }
});
